/**
 * 16-bit incrementer:
 * out = in + 1 (arithmetic addition)
 */

CHIP Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
   // Put you code here:
   
   Not16(in=in, out=notin); 
   Add16(a=true, b=notin, out=preout);
   Not16(in=preout, out=out);

}